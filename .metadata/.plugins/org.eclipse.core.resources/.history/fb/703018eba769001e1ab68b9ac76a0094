package handson.com.cg;

public class SumOfAllDigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="1234";
		String[] str=s.split(";");
		int sum=0;
		for(int i=0;i<str.length;i++) {
			int n=Integer.parseInt(str[i]);
			sum=sum+n;
			System.out.println(n);
			
		}
		
		
		

	}

}
