package Lab1;

public class SumOfNumbers {

	//public static void main(String[] args) {
		static int calculateSum(int n) {
			int sum=0;
			for(int i=0;i<=n;i++) {
				if(n%3==0 || n%5==0) {
					sum=sum+i;
					
				}
			}
			return sum;
		}
		public static void main(String[] args) {
		System.out.println(calculateSum(5));
		}
		
	}


