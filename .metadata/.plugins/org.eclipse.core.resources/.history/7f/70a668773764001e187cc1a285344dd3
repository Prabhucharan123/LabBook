package com.cg;

public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
			int n=Integer.parseInt(args[0]);
			int count=0;
			for(int i=2; i<=n; i++) {
				if(n%i==0) {
					count++;
				}
				if(count==2) {
					System.out.println(i+" is a prime");
				}
				else {
					System.out.println(i+" Not prime");
				}
			}
			
			
			
	}

}
