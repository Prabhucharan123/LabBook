package com.cg;

import java.util.Scanner;

public class Printmonthinwords {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int month=n;
		switch(month) {
		case 1:System.out.println("january");
		break;
		case 2:System.out.println("january");
		break;
		case 3:System.out.println("january");
		break;
		case 4:System.out.println("january");
		break;
		case 5:System.out.println("january");
		break;
		case 6:System.out.println("january");
		break;
		case 7:System.out.println("january");
		break;
		case 8:System.out.println("january");
		break;
		case 9:System.out.println("january");
		break;
		case 10:System.out.println("january");
		break;
		case 11:System.out.println("january");
		break;
		case 12:System.out.println("january");
		break;
		}
		
		

	}

}
