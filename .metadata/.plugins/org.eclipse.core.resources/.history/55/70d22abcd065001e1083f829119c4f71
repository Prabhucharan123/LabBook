	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		System.out.println("please enter a number");
		int i=0;int sum = 0;
		
		while(n<=9) {
			sum=5*n;
			System.out.println(5+" * "+n+" = "+sum);
			n++;
			
			
			
			
			}
			
			System.out.println(5+" * "+n+" = "+sum);
		}
	//	System.out.println(n+" * "+i+" = "+sum);
		

	

}
